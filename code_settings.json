{
	"workbench.colorTheme": "Andromeda Colorizer",
	"terminal.integrated.defaultProfile.linux": "fish",
	"terminal.integrated.profiles.linux": {
		"fish": {
			"path": "host-spawn",
			"args": [
				"fish"
			]
		}
	},
	"editor.renderWhitespace": "all",
